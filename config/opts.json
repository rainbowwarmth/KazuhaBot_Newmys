{
    "desc": "命令总览json,按照顺序进行匹配",
    "command": {
        "admin": {
            "ping": {
                "reg": "^#ping$",
                "fnc": "ping",
                "type": ["DIRECT"],
                "describe": "检测数据库是否正常"
            },
            "status": {
                "reg": "^#(状态|status)$",
                "fnc": "status",
                "type": ["DIRECT"],
                "describe": "查询bot状态"
            }
        },
        "NewYuanShen": {
            "newsContentBBS": {
                "reg": "^#(官方|原神)?(公告|资讯|活动)[0-9]*$",
                "fnc": "newsContentBBS",
                "type": ["GUILD", "DIRECT"],
                "permission": "anyone",
                "describe": "【#官方公告】米游社官方公告"
            },
            "newsListBBS": {
                "reg": "^#(公告|资讯|活动)列表$",
                "fnc": "newsListBBS",
                "type": ["GUILD", "DIRECT"],
                "permission": "anyone",
                "describe": "【#官方公告列表】米游社官方公告列表"
            },
            "changePushTask": {
                "reg": "^#(开启|关闭)公告推送$",
                "fnc": "changePushTask",
                "type": ["GUILD", "DIRECT"],
                "describe": "【#开启/关闭公告推送】"
            },
            "taskPushNews": {
                "reg": "^#测试公告推送$",
                "fnc": "taskPushNews",
                "type": ["GUILD", "DIRECT"],
                "describe": "【#开启/关闭公告推送】"
            }
        }
    }
}
